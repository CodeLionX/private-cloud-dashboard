{
  "name": "private-cloud-dashboard",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server/index.js",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "run-p build:server build:client",
    "prebuild:server": "",
    "build:server": "tsc",
    "prebuild:client": "",
    "build:client": "parcel build src/client/index.html -d dist/static --public-url . --detailed-report",
    "prestart": "npm run build",
    "start": "node .",
    "start:dev": "node .",
    "dev": "run-p dev:client dev:server",
    "dev:client": "parcel watch src/client/index.html -d dist/static --public-url .",
    "dev:server": "nodemon -w src/server -w src/shared -e ts,js,json -x run-s build:server start:dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
