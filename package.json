    "@material-ui/icons": "^4.2.1",
    "axios": "^0.19.0",
    "axios-hooks": "^1.2.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "socket.io": "^2.5.0",
    "socket.io-client": "^2.2.0",
    "winston": "^3.2.1",
    "xterm": "^3.14.5"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
